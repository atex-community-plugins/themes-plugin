<output-template name="com.atex.plugins.themes.ThemeElement.ot" input-template="p.siteengine.ElementOutputTemplate">
	<content-xml>
		<!-- -->
		<contentref group="plugin" name="plugindef">
			<contentid>
				<major>Content</major>
				<externalid>plugins.com.atex.gong.plugins.themes-plugin</externalid>
			</contentid>
		</contentref>
		<component group="polopoly.Content" name="name">Themes output template</component>
		<component group="output/modelKeyMode" name="selected">no-cache</component>
		<component group="output/modelPaths" name="value">m.local.content.contentId;m.request.httpRequest.attributes.filetype</component>
		<contentref group="output/renderer" name="selected">
			<contentid>
				<externalid>p.siteengine.VelocityRenderer</externalid>
			</contentid>
		</contentref>
		<component group="output/renderer" name="templateFileName">com.atex.plugins.themes.ThemeElement.vm</component>
		<component group="preview/controller" name="value">com.polopoly.siteengine.preview.PreviewControllerStandard</component>
		<contentref group="output/controller" name="selected">
			<contentid>
				<externalid>p.siteengine.JavaClassRenderController</externalid>
			</contentid>
		</contentref>
		<component group="output/controller/selected/renderControllerClass" name="value">com.atex.plugins.themes.RenderControllerThemeElement</component>
	</content-xml>

</output-template>
